<?xml version="1.0"?>
<!--    Author: Phoenix Fan
        Date: 11/10/2019 
        Update: Remove base link and the joint connecting base link and torso link.
                Change velocity of each joint to 12.
-->
<robot name="biped_6DOF">

    <material name="white">
        <color rgba="1 1 1 1"/>
    </material>

    <material name="black">
        <color rgba="0.2 0.2 0.2 1"/>
    </material>

    <material name="blue">
        <color rgba="0 0 1 1"/>
    </material>

<!--
    <link name="base" >
        <inertial>
            <origin rpy = "0 0 0" xyz = "0 0 0" />
            <mass value = "0.0001" />
            <inertia ixx = "0.0001" ixy = "0.0" ixz = "0.0" iyy = "0.0001" iyz = "0.0" izz = "0.0001" />
        </inertial>
    </link>
-->

    <link name="torso">
        <visual>
            <geometry>
                <box size="0.2 0.8 0.2"/>
            </geometry>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <material name="white"/>
        </visual>
        <collision>
            <geometry>
                <box size="0.2 0.8 0.2"/>
            </geometry>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <contact_coefficients mu="0.08" />
        </collision>
        <inertial>
            <mass value="32"/>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <inertia ixx = "0.0001" ixy = "0.0" ixz = "0.0" iyy = "0.0001" iyz = "0.0" izz = "0.0001" />
        </inertial>
    </link>

<!--
    <joint name="torso_to_base" type="continuous">
        <parent link="base"/>
        <child link="torso"/>
        <axis xyz="1 0 0"/>
        <origin rpy="0 0 0" xyz="0 0 0"/>
    </joint>
-->

    <link name="right_thigh">
        <visual>
            <geometry>
                <box size="0.2 0.2 0.3"/>
            </geometry>
            <origin rpy="0 0 0" xyz="0 0 -0.15"/>
            <material name="black"/>
        </visual>
        <collision>
            <geometry>
                <box size="0.2 0.2 0.3"/>
            </geometry>
            <origin rpy="0 0 0" xyz="0 0 -0.15"/>
        </collision>
        <inertial>
            <mass value="12"/>
            <origin rpy="0 0 0" xyz="0 0 -0.15"/>
            <inertia ixx = "0.0001" ixy = "0.0" ixz = "0.0" iyy = "0.0001" iyz = "0.0" izz = "0.0001" />
        </inertial>
    </link>
    <joint name="torso_to_right_thigh" type="revolute">
        <parent link="torso"/>
        <child link="right_thigh"/>
        <axis xyz="0 1 0"/>
        <limit effort="500.0" lower="-1.39626" upper="1.39626" velocity="12"/>
        <origin rpy="0 0 0" xyz="0 -0.3 -0.1"/>
    </joint>

    <link name="right_shank">
